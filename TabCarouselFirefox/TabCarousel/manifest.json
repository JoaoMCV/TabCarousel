{
  "manifest_version": 2,
  "name": "Tab Carousel",
  "version": "1.0",
  "description": "Extens√£o para ciclar entre abas abertas.",
  "permissions": [
    "tabs","<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ] 
  },
  "browser_action": { 
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "browser_specific_settings": {
    "gecko": {
        "id": "TabCarousel@teste",
        "strict_min_version": "91.0"
    }
  }
}